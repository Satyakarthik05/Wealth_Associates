      "READ_EXTERNAL_STORAGE",
      "WRITE_EXTERNAL_STORAGE",
      "INTERNET"
    ],
      "package": "com.wealthassociates.app"
    },
    "web": {
      "favicon": "./assets/icon-512.png"
    },
    "extra": {
      "eas": {
        "projectId": "1bf7d0bd-0b16-4b28-85f7-6437c3eaff11"
      }
    },
    "newArchEnabled": true
  }
