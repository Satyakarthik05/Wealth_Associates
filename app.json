{
  "expo": {
    "name": "WealthAssociate",
    "slug": "wealthassociates",
    "scheme": "wealthassociates",
    "version": "1.2.0",
    "orientation": "portrait",
    "icon": "./assets/logo.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/app_icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "url": "https://u.expo.dev/38b6a11f-476f-46f4-8263-95fe96a6d8ca",
      "enabled": true,
      "fallbackToCacheTimeout": 0,
      "checkAutomatically": "ON_LOAD"
    },
    "runtimeVersion": {
      "policy": "sdkVersion"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.wealthassociates.alpha",
      "jsEngine": "hermes",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false,
        "CFBundleURLTypes": [
          {
            "CFBundleURLSchemes": ["wealthassociates"]
          }
        ]
      },
      "ios": "1.2.2"
    },
    "android": {
      "usesCleartextTraffic": true,
      "adaptiveIcon": {
        "foregroundImage": "./assets/app_icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "permissions": [
        "CAMERA",
        "READ_EXTERNAL_STORAGE",
        "WRITE_EXTERNAL_STORAGE",
        "INTERNET",
        "VIBRATE",
        "RECEIVE_BOOT_COMPLETED",
        "com.google.android.c2dm.permission.RECEIVE"
      ],
      "useNextNotificationsApi": true,
      "package": "com.wealthassociates.alpha",
      "intentFilters": [
        {
          "action": "android.intent.action.SEND",
          "data": {
            "mimeType": "image/*"
          },
          "category": [
            "android.intent.category.DEFAULT"
          ]
        },
        {
          "action": "VIEW",
          "data": [
            {
              "scheme": "wealthassociates",
              "host": "*"
            }
          ],
          "category": [
            "BROWSABLE",
            "DEFAULT"
          ]
        }
      ],
      "notification": {
        "icon": "./assets/logo.png",
        "color": "#FF231F7C",
        "defaultMode": "default"
      }
    },
    "web": {
      "favicon": "./assets/icon-512.png"
    },
    "extra": {
      "eas": {
        "projectId": "38b6a11f-476f-46f4-8263-95fe96a6d8ca"
      }
    },
    "plugins": [
      "expo-notifications"
    ]
  }
}